[access "refs/*"]
  read = group repoxplorer-core
  owner = group repoxplorer-ptl
[access "refs/heads/*"]
    label-Code-Review = -2..+2 group repoxplorer-core
    label-Code-Review = -2..+2 group repoxplorer-ptl
    label-Verified = -2..+2 group repoxplorer-ptl
    label-Workflow = -1..+1 group repoxplorer-core
    label-Workflow = -1..+1 group repoxplorer-ptl
    label-Workflow = -1..+0 group Registered Users
    submit = group repoxplorer-ptl
    read = group Registered Users
    read = group repoxplorer-core
[access "refs/tags/*"]
    create = group repoxplorer-ptl
	pushTag = group repoxplorer-ptl
	push = group repoxplorer-ptl
[access "refs/meta/config"]
    read = group Registered Users
    read = group repoxplorer-core
[receive]
    requireChangeId = true
[submit]
    mergeContent = false
    action = rebase if necessary
[project]
        description = RepoXplorer is a small stats and charts utility for GIT repositories
